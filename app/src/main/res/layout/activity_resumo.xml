<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ResumoActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="210dp"
                android:layout_marginTop="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:orientation="vertical">

                <de.codecrafters.tableview.TableView xmlns:table="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/tableView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    table:tableView_columnCount="10" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="210dp"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:orientation="vertical">

                <de.codecrafters.tableview.TableView
                    android:id="@+id/tableView2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:tableView_columnCount="5" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:orientation="vertical">

                <de.codecrafters.tableview.TableView xmlns:table="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/tableView3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    table:tableView_columnCount="9" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:orientation="vertical">

                <de.codecrafters.tableview.TableView xmlns:table="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/tableView6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    table:tableView_columnCount="9" />
            </LinearLayout>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Quantidade Animais com Medidas"
                android:textSize="20dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:orientation="vertical">

                <de.codecrafters.tableview.TableView xmlns:table="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/tableView4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    table:tableView_columnCount="7" />
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="MÃ©dia Medidas"
                android:textSize="20dp" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:orientation="vertical">


                <de.codecrafters.tableview.TableView xmlns:table="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/tableView5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    table:tableView_columnCount="7" />
            </LinearLayout>



            <android.support.design.widget.FloatingActionButton
                android:id="@+id/fab"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|start"
                android:layout_margin="32dp"
                app:srcCompat="@drawable/ic_voltar_white" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>